SELECT * FROM student 
LIMIT 3;


SELECT * FROM Student
ORDER BY Grade DESC
LIMIT 3;


SELECT * FROM Student 
ORDER BY ROLLNO LIMIT 2,5;


SELECT age FROM Student  
ORDER BY age LIMIT 2, 1;